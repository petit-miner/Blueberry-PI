/dts-v1/;
#include "sun8i-v3s.dtsi"
#include "sunxi-common-regulators.dtsi"
#include <dt-bindings/input/input.h>
/ {

	model = "Blueberry Pi";
	compatible = "blueberrypi", "allwinner,sun8i-v3s";

	
	aliases {
		serial0 = &uart0;
		ethernet0 = &emac;
		
	};

	chosen {
		stdout-path = "serial0:115200n8";
		
		};

	leds {
	compatible = "gpio-leds";

	act_led {
		label = "act_led:green:usr";
		gpios = <&pio 1 2 GPIO_ACTIVE_HIGH>; /* PB2 */
		linux,default-trigger = "mmc0";
		default-state = "off";
	};
	};
	
	bridge {
		compatible = "dumb-vga-dac";
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";
		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;

				vga_bridge_in: endpoint {
					remote-endpoint = <&tcon0_out_vga>;
				};
			};

			port@1 {
				reg = <1>;

				vga_bridge_out: endpoint {
					remote-endpoint = <&vga_con_in>;
				};
			};
		};
	};

	vga {
		compatible = "vga-connector";

		port {
			vga_con_in: endpoint {
				remote-endpoint = <&vga_bridge_out>;
			};
		};
	};

};

&fb0 {
	status = "okay";
};


&de0 {
	status = "okay";
};

&display_clocks {
	status = "okay";
};

&mixer0 {
	status = "okay";
};


&tcon0 {
	pinctrl-names = "default";
	pinctrl-0 = <&rgb_pins>;
	status = "okay";
};

&tcon0_out {
	tcon0_out_vga: endpoint@0 {
		reg = <0>;
		remote-endpoint = <&vga_bridge_in>;
	};
};


&ehci0 {
        status = "okay";
}; 

&ohci0 {
        status = "okay";
}; 

&usb_otg {
	dr_mode = "otg";
	status = "okay";
};

&usbphy {
	usb0_id_det-gpio = <&pio 5 6 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&emac {
	allwinner,leds-active-high;
	status = "okay";
};

&mmc0 {
	pinctrl-0 = <&mmc0_pins>;
	pinctrl-names = "default";
	broken-cd;
	bus-width = <4>;
	vmmc-supply = <&reg_vcc3v3>;
	status = "okay";
};

&uart0 {
	pinctrl-0 = <&uart0_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&spi0 {
	status = "okay";
};

&i2c0 {
	pinctrl-0 = <&i2c0_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&lradc {
	vref-supply = <&reg_vcc3v0>;
	status = "okay";

	button@200 {
		label = "Volume Up";
		linux,code = <KEY_VOLUMEUP>;
		channel = <0>;
		voltage = <200000>;
	};

	button@400 {
		label = "Volume Down";
		linux,code = <KEY_VOLUMEDOWN>;
		channel = <0>;
		voltage = <400000>;
	};

	button@600 {
		label = "Select";
		linux,code = <KEY_SELECT>;
		channel = <0>;
		voltage = <600000>;
	};

	button@800 {
		label = "Start";
		linux,code = <KEY_OK>;
		channel = <0>;
		voltage = <800000>;
	};
};

&codec {
	allwinner,audio-routing = "Headphone", "HP", "Headphone", "HPCOM", "Mic", "MIC1";
	status = "okay";
};
		
&pio {

	rgb_pins: rgb_pins@0 {
	        pins = "PE0", "PE1", "PE2", "PE3", "PE4",
		       "PE5", "PE6", "PE7", "PE8", "PE9",
		       "PE10", "PE11", "PE12", "PE13", "PE14",
		       "PE15", "PE16", "PE17", "PE18", "PE19",
		       "PE23", "PE24";
	        function = "lcd";
    	};

	i2c0_pins: i2c0 {
		pins = "PB6", "PB7";
		function = "i2c0";
	};

	i2c1_pins: i2c1 {
		pins = "PE21", "PE22";
		function = "i2c1";
	};
	

	uart0_pins: uart0@0 {
	 	pins = "PB8", "PB9";
		function = "uart0";
	};

};

